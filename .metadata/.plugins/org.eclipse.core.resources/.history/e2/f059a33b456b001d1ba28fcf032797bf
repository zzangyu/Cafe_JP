function fetchPage(){
	var items = "";
    $.ajax({
  	url: 'https://webservice.recruit.co.jp/hotpepper/gourmet/v1/?key=79eb47654869941a&keyword=cafe&large_area=Z011&format=jsonp',
  	type: 'GET',
  	dataType: 'jsonp',
  	jsonpCallback: 'callback',
	large_area : 'Z011',
	keyword : 'cafe',	
	}).done(function(data) {
		var shop = data;
		console.log(shop)
	}).fail(function(data) {
  		var shop = data;
		console.log(shop)
	});
}

